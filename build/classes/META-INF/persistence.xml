<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="Subject_Outline_SummaryPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>omanyteDB.User</class>
    <class>omanyteDB.Password</class>
    <class>omanyteDB.SosAssessmentGroup</class>
    <class>omanyteDB.UserSubjects</class>
    <class>omanyteDB.SosAssessments</class>
    <class>omanyteDB.SubjectOutlineSummary</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:derby://localhost:1527/sos_db"/>
      <property name="javax.persistence.jdbc.user" value="omanyte"/>
      <property name="javax.persistence.jdbc.driver" value="org.apache.derby.jdbc.ClientDriver"/>
      <property name="javax.persistence.jdbc.password" value="bake"/>
    </properties>
  </persistence-unit>
</persistence>
